@model int
@{
    ViewData["Title"] = "تقييمات  الملف الصوتي " + " " + ViewBag.audioName;
}

<!-- Page header start -->
<div class="page-header">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">الرئيسية</li>
        <li class="breadcrumb-item">ملفات الصوت</li>
        <li class="breadcrumb-item active">@("تقييمات  الملف الصوتي "+" "+ ViewBag.audioName)</li>
    </ol>


</div>
<!-- Page header end -->
<!-- Content wrapper start -->
<div class="content-wrapper">

    <!-- Row start -->
    <div class="row gutters">

        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="card h-100">

                <div class="card-body">
                    <div class="card-form">
                        <div class="card-header">
                            <div class="card-title">
                                <i class="icon-layers2"></i> &nbsp; إدارة تقييمات  الملف الصوتي

                            </div>

                        </div>
                        <div class="card-body">

                            <div class="row gutters">



                                <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12  p-t-20">

                                    <div class="card-form">

                                        <div class="card-body">
                                            <div id="divListAudioRate">
                                                @Html.Action("ListAudioRate", "Audio", new { page = 1, audioId = Model })
                                            </div>

                                        </div>
                                    </div>

                                </div>

                            </div>

                        </div>

                    </div>




                </div>
            </div>
        </div>
    </div>

</div>
<!-- Content wrapper end -->
@section Scripts{
    <script src="~/js/Views/Audio.js" asp-append-version="true"></script>
}