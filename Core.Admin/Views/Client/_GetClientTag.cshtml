@model IList<ClientTagVM>

    <div class="row gutters">
        @foreach (var item in Model)
        {
            <div class="col-xl-3 col-lg-3 col-md-3 col-sm-12 col-12">
                <div class="card-deck">
                    <div class="card">
                        <img class="card-img-top" src="@(string.IsNullOrEmpty(item.Image)?"/images/tag.jpg":"/Attachments/Images/"+item.Image)" alt="Card image cap">
                        <div class="card-body">
                            <h5 class="card-title2">@item.Name</h5>

                        </div>
                    </div>


                </div>
            </div>
        }

    </div>
